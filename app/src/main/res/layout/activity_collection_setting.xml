<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include  layout="@layout/head_include_layout"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:paddingTop="20dp">

            <View
                android:layout_width="700dp"
                android:layout_height="2dp"
                android:background="#E6E6E6" />

            <LinearLayout
                android:layout_width="700dp"
                android:layout_height="430dp"
                android:orientation="horizontal">

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#E6E6E6" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/ll_pop1_ll"
                        android:layout_width="696dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:padding="5dp"
                            android:text="测试编号"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:id="@+id/ll_pop1"
                            android:layout_width="400dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/frame_textview"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_syrwbh"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:padding="5dp"
                                android:textSize="20dp" />

                            <TextView
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/layer_list_inverted_triangle"
                                android:visibility="gone" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="696dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="30dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:padding="5dp"
                            android:text="模板名称"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:id="@+id/ll_pop2"
                            android:layout_width="400dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/frame_textview"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_mbmc"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"

                                android:padding="5dp"
                                android:textSize="20dp" />

                            <TextView
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/layer_list_inverted_triangle" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="696dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="30dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:padding="5dp"
                            android:text="生育期"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:id="@+id/ll_pop3"
                            android:layout_width="400dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/frame_textview"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_syq"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"

                                android:padding="5dp"
                                android:textSize="20dp" />

                            <TextView
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/layer_list_inverted_triangle" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="696dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="30dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:padding="5dp"
                            android:text="观测方式"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:id="@+id/ll_pop4"
                            android:layout_width="400dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/frame_textview"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_gcfs"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"

                                android:padding="5dp"
                                android:textSize="20dp" />

                            <TextView
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/layer_list_inverted_triangle" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="696dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="30dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:padding="5dp"
                            android:text="性状"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:id="@+id/ll_pop5"
                            android:layout_width="400dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/frame_textview"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_xz"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"

                                android:padding="5dp"
                                android:textSize="20dp" />

                            <TextView
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/layer_list_inverted_triangle" />
                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#E6E6E6" />
            </LinearLayout>

            <View
                android:layout_width="700dp"
                android:layout_height="2dp"
                android:background="#E6E6E6" />

            <LinearLayout
                android:gravity="center"
                android:background="#F2F2F2"
                android:layout_width="700dp"
                android:layout_height="50dp">
                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#E6E6E6" />
                <View
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"/>
                <Button
                    android:id="@+id/bt_queren"
                    android:gravity="center"
                    android:text="确认"
                    android:background="@drawable/shape_small_label_signin_"
                    android:layout_width="100dp"
                    android:layout_height="40dp" />
                <View
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"/>
                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#E6E6E6" />
            </LinearLayout>

            <View
                android:layout_width="700dp"
                android:layout_height="2dp"
                android:background="#E6E6E6" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
